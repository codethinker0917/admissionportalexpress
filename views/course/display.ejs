<%- include('../header.ejs')%>
    <div class="container mt-5">
        <h1 class="text-center pt-5">Your Course's</h1>

        <% if(msg.length>0){ %> 

            <div class="alert alert-success"><%= msg %></div>

      <%  }%>

        <table class="table">
            <tr>
                <th>id</th>
                <th>Name</th>
                <th>Email</th>
                <th>Phone</th>
                <th>Action</th>
            </tr>
            <%d.forEach((item)=>{%>
            <tr>
            <td><%=item._id%></td>
            <td><%=item.name%></td>
            <td><%=item.email%></td>
            <td><%=item.phone%></td>
            <td>
                <a href="/course_view/<%=item._id%>" class="btn btn-info">View</a>
                <a href="/course_edit/<%= item._id %>" class="btn btn-primary">Edit</a>
                <a href="/course_delete/<%= item._id %>" class="btn btn-danger">Delete</a>
            </td>
        </tr>
        <%})%>
        </table>
    </div>
    <%- include('../footer.ejs')%>